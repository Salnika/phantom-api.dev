{
  "name": "phantom-api-client",
  "version": "0.2.0-alpha",
  "bin": {
    "phantom-migration": "dist/migration-cli.js",
    "phantom-seed": "dist/seed-cli.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "knip": "^5.61.3",
    "oxlint": "^1.11.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.9",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.8.3"
  },
  "main": "dist/index.js",
  "projectConfig": {
    "type": "package",
    "artifactName": "salnika-phantom-api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salnika/phantom-api.dev.git"
  },
  "license": "MIT",
  "keywords": [
    "api",
    "backend",
    "dynamic",
    "typescript",
    "client",
    "orm",
    "database",
    "phantom"
  ],
  "homepage": "https://github.com/salnika/phantom-api.dev",
  "bugs": {
    "url": "https://github.com/salnika/phantom-api.dev/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "doc": "typedoc",
    "knip": "knip --config knip.json",
    "lint": "oxlint ./src",
    "prepare-dist": "node scripts/prepare-dist.cjs",
    "prepublish": "yarn build && yarn prepare-dist",
    "publish:npm": "cd dist && npm publish",
    "publish:dry": "cd dist && npm publish --dry-run",
    "test": "echo \"Tests to implement for client-package\"",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
